@model IEnumerable<Rocky.Models.Tests>
@{
    ViewData["Title"] = "Категории";
}
<div class = "container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">Список тестов</h2>
        </div>
        <div class="col-6 text-right">
            <a asp-controller="Tests" asp-action="Create" class="btn btn-primary">
            Создать новый тест
            </a>
        </div>
    </div>

<br />
<br />
@if(Model.Count() >0){
    <table class="table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>
                    Название тестов
                </th>
               
               
            </tr>

        </thead>
        <tbody>
            @foreach (var obj in Model)
           {
               <tr>
                   <td width=50%>@obj.Name</td>
                   
                   <td>
                   </td>
               </tr>

               
           }
        </tbody>
    </table>
}
else{
    <p>Категории не существуют</p>
}
</div>


